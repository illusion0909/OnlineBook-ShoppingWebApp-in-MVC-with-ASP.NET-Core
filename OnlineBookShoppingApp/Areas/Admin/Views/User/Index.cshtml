<div class="row">
    <div class="col-9">
        <h2 class="text-dark">User List</h2>
    </div>
    <div class="col-3">
        <a asp-area="Identity" class="btn btn-success" asp-page="/Account/Register">
            <i class="fas fa-plus"> &nbsp; Create New User</i>

            </a>
    </div>
</div>

<div class="border rounded p-3 m-3">
    @*<table id="tblData" class="table table-striped table-bordered">*@
    <table id="tblData" class="table table-striped table-bordered table-active table-dark bg-dark text-white">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Company</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts{
    <script src="~/js/User.js"></script>
}


